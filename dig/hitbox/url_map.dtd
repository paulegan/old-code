<!ELEMENT url_map (site+) >
<!ELEMENT site (path+) >
<!ATTLIST site host CDATA #IMPLIED
               root CDATA #IMPLIED >
<!ELEMENT path ((country,unit,brand,site_name?)?) >
<!ATTLIST path pattern CDATA #REQUIRED
               order CDATA #REQUIRED >
<!ELEMENT country (#PCDATA) >
<!ELEMENT unit (#PCDATA) >
<!ELEMENT brand (#PCDATA) >
<!ELEMENT site_name (#PCDATA) >
